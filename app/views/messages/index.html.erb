<h1>Inbox</h1>


<table class="table table-striped">
  <thead class="thead thead-default">
    <tr>
      <td>Sender</td>
      <td>Subject</td>
    </tr>
  </thead>
  <tbody>
  <% @messages.each do |message|  %>
    <tr>
      <td><%= message.sender %></td>
      <td><%= message.body %></td>
    </tr>
  <% end %>
  </tbody>
</table>

