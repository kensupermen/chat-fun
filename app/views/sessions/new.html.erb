<h1>Sessions#new</h1>
<p>Find me in app/views/sessions/new.html.erb</p>


<%= form_tag sessions_path, method: 'post' do %>
  <%= text_field_tag :email, params[:email], class: 'form-control', placeholder: "Email"  %>
  <%= text_field_tag :password, nil,  class: 'form-control', placeholder: "Enter password"  %>

  <br>
  <%= submit_tag "Log In", data:{disable_with: "Signing in..."}, class: 'btn btn-primary' %>
<% end %>
